<div class="content">
	<header>
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
	          <div id="inputs-box">
	            <%= text_field_tag :message, nil, placeholder:"text-to-speech on submit" %>
	            <input type="submit" name="tts-submit" id="tts-submit" value="submit">
	          </div>
					</div>
				</div>
			</div>
		</header>

	<div class="container">
		<div class="settings-box">
			<div class="row text-center">
				<div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
					<p class="setting-label">WARM</p>
					<button id="minus-warm" type="button" class="minus">-</button>
					<input id="timer-warm" autocomplete="off" disabled type="text" value="00:10"></input>
					<button id="plus-warm" type="button" class="plus">+</button>
				</div>

				<div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
					<p class="setting-label">WORK</p>
					<button id="minus-work" type="button" class="minus">-</button>
					<input id="timer-work" autocomplete="off" disabled type="text" value="00:00"></input>
					<button id="plus-work" type="button" class="plus">+</button>
				</div>

				<div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
					<p class="setting-label">REST</p>
					<button id="minus-rest" type="button" class="minus">-</button>
					<input id="timer-rest" autocomplete="off" disabled type="text" value="00:00"></input>
					<button id="plus-rest" type="button" class="plus">+</button>
				</div>

				<div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
					<p class="setting-label">COOL</p>
					<button id="minus-cool" type="button" class="minus">-</button>
					<input id="timer-cool" autocomplete="off" disabled type="text" value="00:00"></input>
					<button id="plus-cool" type="button" class="plus">+</button>
				</div>

				<div class="col-lg-2 col-md-3 col-sm-4 col-xs-12">
					<p class="setting-label">SET</p>
					<button id="minus-set" class="minus" type="button">-</button>
					<input id="number-set"autocomplete="off" disabled type="text" value="1">
					<button id="plus-set" class="plus">+</button>
				</div>
			</div>

			<div class="row text-center">
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
					<p class="setting-label">TOTAL TIME</p>
					<input id="total-time" autocomplete="off" disabled type="text" value="00:00">
				</div>
			</div>

			<div class="row text-center">
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
					<button id="start-button" type="button">Start</button>
					<button id="stop-button" type="button">Stop</button>
					<button id="reset-button" type="button">Reset</button>
				</div>
			</div>
		</div>
	</div>

